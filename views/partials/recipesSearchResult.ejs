<% if (data.results){ %>

<section id="recipe-result-section">


    <% for (i in data.results){ %>
		<div class="recipe-wrapper">
			<img class="recipe-img" src="<%= data.results[i].feature_image_url %>" alt="Bilde av matrett">
			<h5 class="recipe-title"><%= data.results[i].title %></h5>
			<p class="recipe-difficulty"><%= data.results[i].difficulty_string %></p>
			<p class="recipe-duration"><%= data.results[i].cooking_duration_string %></p>
		</div>
    <% } %>

</section>
<% } %>